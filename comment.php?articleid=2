function doCheck(){

// 检测表单的有效性
// 如：标题不能为空，内容不能为空，等等....
if (myform.username.value=="") {
alert("请输入姓名!");
myform.username.focus();
return false;
}

if (myform.content.value=="") {
alert("请输入评论内容!");
myform.content.focus();
return false;
}
return true;
}
</script>
