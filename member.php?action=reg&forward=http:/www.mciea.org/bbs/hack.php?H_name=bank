function check()
{
          if (myform.username.value == ""){
   		alert("请输入您的昵称!");
myform.username.focus();
return false ;
          }
  if (trim(myform.username.value).length<2 || trim(myform.username.value).length>20){
    alert("用户名不得小于4个字符或大于20个字符！");
myform.username.focus();
return false;
  }   
  if (trim(myform.password.value).length<6 || trim(myform.password.value).length>32 ){
    alert("密码不能为空且不得少于6位或超过32位！");
myform.password.focus();
return false;
  }
  if (trim(myform.password.value) != trim(myform.pwdconfirm.value)){
    alert("两次密码不一致!");
myform.password.focus();
                myform.password.value=myform.pwdconfirm.value="";
return false;
  }
         if(myform.question.value == ""){
   		alert("请填写密码提示问题!");
myform.question.focus();
return false ;
        }
        if(myform.answer.value == ""){
   		alert("请填写密码提示问题答案!");
myform.answer.focus();
return false ;
        }
       if(validEmail(myform.email.value)==false) {
      alert('请输入有效的电子邮件地址');
      myform.email.focus();
      return false;
       }
       if(myform.msn.value!="" && validEmail(myform.msn.value)==false) {
   alert('请输入有效的MSN');
   myform.msn.focus();
   return false;
       }
   return true;     
 }
